# For local development with Vite proxy, leave VITE_API_URL empty.
VITE_API_URL=

# Vite dev server configuration
VITE_DEV_PORT=5173

# Backend API target used by Vite proxy for /api requests in development
VITE_API_PROXY_TARGET=http://localhost:5001

# Optional Google OAuth client ID for auth screens
VITE_GOOGLE_CLIENT_ID=
